{
  "name": "fallacymania-web",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3022",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "typescript": "^4.4.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "client": "react-scripts start",
    "build": "react-scripts build",
    "devserver": "nodemon --watch 'src' --ext '*' --ignore 'src/client' --exec 'ts-node --project src/server/tsconfig.json src/server/start.ts'",
    "server": "NODE_ENV=production ts-node --project src/server/tsconfig.json src/server/start.ts",
    "start": "concurrently \"npm run client\" \"npm run devserver\"",
    "prod": "npm run build && npm run server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1"
  }
}
